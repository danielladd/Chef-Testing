setuid vault
setgid vault
limit nofile 65536 65536
chdir /var/vault
exec java -server -d64 -Xms<%= node[:chub_vault][:java_heap_size] %> -Xmx<%= node[:chub_vault][:java_heap_size] %> -Djava.net.preferIPv4Stack=true -Dcom.sun.management.jmxremote.port=6006 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -jar /opt/vault/vault.jar server /etc/vault/vault.yaml
respawn
start on runlevel [2345]
stop on runlevel [016]
