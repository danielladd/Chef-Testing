setuid census
setgid census
chdir /var/census
exec java -server -d64 -Xms<%= node[:chub_census][:java_heap_size] %> -Xmx<%= node[:chub_census][:java_heap_size] %> -Djava.net.preferIPv4Stack=true -jar /opt/census/census.jar server /etc/census/census.yaml
respawn
start on runlevel [2345]
stop on runlevel [016]
