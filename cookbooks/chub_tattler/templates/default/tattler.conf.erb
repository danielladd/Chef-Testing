setuid tattler
setgid tattler
chdir /var/tattler
exec java -server -d64 -Xms<%= node[:chub_tattler][:java_heap_size] %> -Xmx<%= node[:chub_tattler][:java_heap_size] %> -Djava.net.preferIPv4Stack=true -Dcom.sun.management.jmxremote.port=6006 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -jar /opt/tattler/tattler.jar server /etc/tattler/tattler.yaml
respawn
start on runlevel [2345]
stop on runlevel [016]