security {
    shiro {
        cas {
            serverUrl = "<%= node["chub-plaza"]["cas_server_url"] %>"
            serviceUrl = "<%= node["chub-plaza"]["cas_service_url"] %>"
            failureUrl = "<%= node["chub-plaza"]["cas_failure_url"] %>"
        }
    }
}
census {
    url = "<%= node["chub-plaza"]["census_url"] %>"
}
<% if node["graphite"] -%>
metrics {
    <% if node["graphite"]["host"] -%>
    graphiteHost = "<%= node["graphite"]["host"] %>"
    <% end -%>
    <% if node["graphite"]["port"] -%>
    graphitePort = <%= node["graphite"]["port"] %>
    <% end -%>
    <% if node.chef_environment != "_default" -%>
    environment = "<%= node.chef_environment %>"
    <% end -%>
}
<% end -%>
