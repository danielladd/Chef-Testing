template: Hello, %s!

defaultName: Stranger

server:
  applicationConnectors:
    - type: http
      port: 8080

metrics:
  reporters:
    - type: hub-graphite
      host: "<%= node[:slm][:graphite][:host] %>"
      port: <%= node[:slm][:graphite][:port] %>
      environment: "<%= node.chef_environment %>"

logging:
  level: INFO

  loggers:
    "org.hibernate": WARN