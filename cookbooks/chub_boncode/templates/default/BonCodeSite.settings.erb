<Settings>
    <Server><%= node[:chub_boncode][:settings][:hostname] %></Server>
    <Port><%= node[:chub_boncode][:settings][:tomcatAjpPort] %></Port>
    <PacketSize><%= node[:chub_boncode][:settings][:packetSize]%></PacketSize>
    <EnableHTTPStatusCodes><%= node[:chub_boncode][:settings][:enableHTTPStatusCodes] %></EnableHTTPStatusCodes>
    <EnableHeaderDataSupport><%= node[:chub_boncode][:settings][:enableHeaderDataSupport] %></EnableHeaderDataSupport>
    <EnableRemoteAdmin><%= node[:chub_boncode][:settings][:enableRemoteAdmin] %></EnableRemoteAdmin>
    <AllowEmptyHeaders><%= node[:chub_boncode][:settings][:allowEmptyHeaders] %></AllowEmptyHeaders>
    <MaxConnections><%= node[:chub_boncode][:settings][:maxConnections] %></MaxConnections>
    <FlushThreshold><%= node[:chub_boncode][:settings][:flushThreshold] %></FlushThreshold>
    <WriteTimeOut><%= node[:chub_boncode][:settings][:writeTimeOut] %></WriteTimeOut>
    <LogLevel><%= node[:chub_boncode][:settings][:logLevel] %></LogLevel>
    <LogDir><%= node[:chub_boncode][:settings][:logDir] %></LogDir>
</Settings>

