{
  "setupDatabase": {
    "serverName": "<%= node[:chub_mc_webservice][:flight_config][:setupDatabase][:serverName] %>",
    "user": "<%= node[:chub_mc_webservice][:flight_config][:setupDatabase][:user] %>",
    "password": "<%= node[:chub_mc_webservice][:flight_config][:setupDatabase][:password] %>",
    "databaseName": "<%= node[:chub_mc_webservice][:flight_config][:setupDatabase][:databaseName] %>"
  },
  "testDatabase": {
    "serverName": "<%= node[:chub_mc_webservice][:flight_config][:testDatabase][:serverName] %>",
    "user": "<%= node[:chub_mc_webservice][:flight_config][:testDatabase][:user] %>",
    "password": "<%= node[:chub_mc_webservice][:flight_config][:testDatabase][:password] %>",
    "databaseName": "<%= node[:chub_mc_webservice][:flight_config][:testDatabase][:databaseName] %>"
  },
  "productionDatabase": {
    "serverName": "<%= node[:chub_mc_webservice][:flight_config][:prodDatabase][:serverName] %>",
    "user": "<%= node[:chub_mc_webservice][:flight_config][:prodDatabase][:user] %>",
    "password": "<%= node[:chub_mc_webservice][:flight_config][:prodDatabase][:password] %>",
    "databaseName": "<%= node[:chub_mc_webservice][:flight_config][:prodDatabase][:databaseName] %>"
  },
  "payloadGeneratorSettings": {
    "templateSourcePath": "<%= node[:chub_mc_webservice][:flight_config][:pGen][:templateSourcePath] %>",
    "autoprocRootPath": "<%= node[:chub_mc_webservice][:flight_config][:pGen][:autoprocRootPath] %>",
    "ftpRootPath": "<%= node[:chub_mc_webservice][:flight_config][:pGen][:ftpRootPath] %>"
    },
    "swaggerConfiguration": {
        "version": "1.0.0",
        "basePath": "http://localhost:8080"
    },
    "logging": {
        "appenders": [
            {
                "type": "file",
                "currentLogFilename": "/var/log/flightws.log",
                "archivedLogFilenamePattern": "/var/log/flightws-%d.log.gz",
                "archivedFileCount": "7",
                "timeZone": "US/Eastern"
            }
        ]
    }
}