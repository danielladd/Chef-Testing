dbHost: "<%= node[:chub_mc_webservice][:platform_config][:db] %>.nexus.commercehub.com" 
dbPort: <%= node[:chub_mc_webservice][:platform_config][:db_port] %> 
logging:
  appenders:
    - type: console
      threshold: INFO
      target: stderr
    - type: file
      currentLogFilename: /var/log/platform-webservice.log
      archivedLogFilenamePattern: /var/log/platform-webservice-%d.log.gz
      archivedFileCount: 5
      timeZone: UTC