{
    "spring": {
        "appContextType": "web",
        "configLocationsType": "classpath",
        "configLocations": [
            "applicationContext.xml",
            "dropwizard-service.xml",
            "dsm-prod-spring-jdo-config.xml",
            "orgDataDir-config.xml",
            "com/commercehub/core/odd/odd-applicationContext.xml"
        ],

        "resources": [
            "contactResource",
            "dynamicProfileResource",
            "productResource",
            "relationshipResource"
        ],

        "healthChecks": [
            "oddDatabaseHealthCheck",
            "onboarderDatabaseHealthCheck",
            "partnerSetupDatabaseHealthCheck"
        ],

        "filters": {
            "OpenPersistenceManagerInViewFilter": {
                "clazz": "org.springframework.orm.jdo.support.OpenPersistenceManagerInViewFilter",
                "url": "/*",
                "param": {
                    "persistenceManagerFactoryBeanName": "internalPersistenceManagerFactory"
                }
            },
            "dsmProdOpenPersistenceManagerInViewFilter": {
                "clazz": "org.springframework.orm.jdo.support.OpenPersistenceManagerInViewFilter",
                "url": "/*",
                "param": {
                    "persistenceManagerFactoryBeanName": "dsmProdInternalPersistenceManagerFactory"
                }
            },
            "onboarderOpenPersistenceManagerInViewFilter": {
                "clazz": "org.springframework.orm.jdo.support.OpenPersistenceManagerInViewFilter",
                "url": "/*",
                "param": {
                    "persistenceManagerFactoryBeanName": "onboarderInternalPersistenceManagerFactory"
                }
            },
            "OpenEntityManagerInViewFilter": {
                "clazz": "org.springframework.orm.jpa.support.OpenEntityManagerInViewFilter",
                "url": "/*",
                "param": {
                    "entityManagerFactoryBeanName": "orgDataDir_EntityManagerFactory"
                }
            },
            "OrgDataDirAuditRequestContextFilter": {
                "clazz": "com.commercehub.connmgr.filter.OrgDataDirAuditRequestContextFilter",
                "url": "/*"
            }
        }
    }
}




























