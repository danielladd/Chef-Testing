server {
  listen   3000;
  server_name  localhost;

  access_log  /var/log/nginx/yuicombo.access.log;

  location / {
    proxy_pass http://localhost:8000;
    proxy_cache combo;
    proxy_cache_use_stale error timeout invalid_header updating
                            http_500 http_502 http_503 http_504;
  }
}