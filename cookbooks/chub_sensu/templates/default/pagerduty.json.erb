{
  "pagerduty": {
    "api_key": "<%= @api_key %>"
    <% if @keys %>
    <% @keys.each_with_index do |pduty, index| %>
      "<%= pduty.key %>":{
        <% if pduty[:api_key] %>
        "api_key": "<%= pduty[:api_key] %>"
    }<% if !@keys.size == index %>,<% end %>
      <% end %>
    <% end %>
    <% end %>
  },
  "handlers": {
    "pagerduty": {
      "command": "/etc/sensu/handlers/pagerduty.rb",
      "type": "pipe"
    }
  }
}
