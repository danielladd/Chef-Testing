# default: on
# description: redis role check
service redischk
{
        flags           = REUSE
        socket_type     = stream
        port            = 12345
        wait            = no
        user            = nobody
        server          = /opt/check_redis_master/redis-role.sh
        log_on_failure  += USERID
        disable         = no
        only_from       = 0.0.0.0/0
        per_source      = UNLIMITED
        type = UNLISTED
}