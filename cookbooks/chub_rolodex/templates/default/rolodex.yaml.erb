# /etc/rolodex/rolodex.yaml
#
# Generated by Chef

http:
  port: <%= node[:slm][:rolodex][:port] %>
  adminPort: <%= node[:slm][:rolodex][:admin_port] %>
  requestLog:
    timeZone: UTC
    console:
      enabled: false
    file:
      enabled: true
      threshold: Level.ALL
      currentLogFilename: /var/log/upstart/rolodex_requests.log
      archive: true
      archivedLogFilenamePattern: /var/log/upstart/rolodex_requests-%d.log.gz
      archivedFileCount: 5
logging:
  rootLevel: INFO
  console:
    enabled: false
  file:
    enabled: true
    currentLogFilename: /var/log/upstart/rolodex_file.log
    archive: true
    archivedLogFilenamePattern: /var/log/upstart/rolodex_file-%d.log.gz
    archivedFileCount: 5