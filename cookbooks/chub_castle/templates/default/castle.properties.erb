host.name=<%= @castle_host_name %>
defaultService.url=<%= node[:chub_castle][:default_service_url] %>
forgotPassword.url=<%= node[:chub_castle][:forgot_password_url] %>
vault.url=<%= node[:chub_castle][:vault_url] %>
<% if node[:graphite] -%>
 <% if node[:graphite][:host] -%>
graphite.host=<%= node[:graphite][:host] %>
 <% end -%>
 <% if node.chef_environment != "_default" -%>
graphite.env=<%= node.chef_environment %>
 <% end -%>
<% end -%>
<% if node[:chub_castle] && node[:chub_castle][:hazelcast] -%>
hazelcast.group=<%= node[:chub_castle][:hazelcast][:group] %>
hazelcast.password=<%= node[:chub_castle][:hazelcast][:password] %>
 <% if node[:chub_castle][:hazelcast][:network_port] -%>
hazelcast.network-port=<%= node[:chub_castle][:hazelcast][:network_port] %>
 <% end -%>
 <% if node[:chub_castle][:hazelcast][:multicast_group] -%>
hazelcast.multicast-group=<%= node[:chub_castle][:hazelcast][:multicast_group] %>
 <% end -%>
 <% if node[:chub_castle][:hazelcast][:multicast_port] -%>
hazelcast.multicast-port=<%= node[:chub_castle][:hazelcast][:multicast_port] %>
 <% end -%>
 <% if node[:chub_castle][:hazelcast][:backup_count] -%>
hazelcast.backup-count=<%= node[:chub_castle][:hazelcast][:backup_count] %>
 <% end -%>
hazelcast.stTimeout=<%= node[:chub_castle][:hazelcast][:service_ticket_timeout] %>
hazelcast.tgtTimeout=<%= node[:chub_castle][:hazelcast][:ticket_granting_ticket_timeout] %>
<% end -%>
