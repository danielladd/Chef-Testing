<?xml version="1.0" encoding="UTF-8"?>

<project>
  <node name="<%= node[:fqdn] %>" description="Rundeck server node" tags="" hostname="<%= node[:fqdn] %>" osArch="amd64" osFamily="unix" osName="<%= node[:os] %>" osVersion="<%= node[:os_version] %>" username="rundeck-ssh"/>
<% if @nodes %>
<% @nodes.each do |box| %>
  <node name="<%= box[:name] %>" description="<%= box[:description] %>" tags="<%= box[:tags] %>" hostname="<%= box[:hostname] %>" osArch="<%= box[:osArch] %>" osFamily="<%= box[:osFamily] %>" osName="<%= box[:osName] %>" osVersion="<%= box[:osVersion] %>" username="rundeck-ssh"/>
<% end %>
<% end %>
</project>
