host.name=<%= @castle_host_name %>
vault.url=<%= node["chub-castle"]["vault.url"] %>
<% if node["graphite"] -%>
 <% if node["graphite"]["host"] -%>
graphite.host=<%= node["graphite"]["host"] %>
 <% end -%>
 <% if node.chef_environment != "_default" -%>
graphite.env=<%= node.chef_environment %>
 <% end -%>
<% end -%>	
<% if node["chub-castle"] && node["chub-castle"]["hazelcast"] -%>
hazelcast.group=<%= node["chub-castle"]["hazelcast"]["group"] %>
hazelcast.password=<%= node["chub-castle"]["hazelcast"]["password"] %>
 <% if node["chub-castle"]["hazelcast"]["network-port"] -%>
hazelcast.network-port=<%= node["chub-castle"]["hazelcast"]["network-port"] %>
 <% end -%>
 <% if node["chub-castle"]["hazelcast"]["multicast-group"] -%>
hazelcast.multicast-group=<%= node["chub-castle"]["hazelcast"]["multicast-group"] %>
 <% end -%>
 <% if node["chub-castle"]["hazelcast"]["multicast-port"] -%>
hazelcast.multicast-port=<%= node["chub-castle"]["hazelcast"]["multicast-port"] %>
 <% end -%>
 <% if node["chub-castle"]["hazelcast"]["backup-count"] -%>
hazelcast.backup-count=<%= node["chub-castle"]["hazelcast"]["backup-count"] %>
 <% end -%>
hazelcast.stTimeout=<%= node["chub-castle"]["hazelcast"]["stTimeout"] %>
hazelcast.tgtTimeout=<%= node["chub-castle"]["hazelcast"]["tgtTimeout"] %>
<% end -%>
